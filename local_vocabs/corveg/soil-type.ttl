cv-corveg:soil-type a owl:Ontology ;
    owl:imports <http://www.w3.org/2004/02/skos/core>,
        sosa: .

cv-corveg:soil-type-a a skos:Concept,
        sosa:Result ;
    skos:definition "Sands." ;
    skos:hiddenLabel "1" ;
    skos:inScheme cv-corveg:soil-types ;
    skos:notation "A" ;
    skos:prefLabel "Sands" .

