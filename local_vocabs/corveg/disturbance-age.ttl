cv-corveg:disturbance-age a owl:Ontology ;
    owl:imports <http://www.w3.org/2004/02/skos/core>,
        sosa: .

cv-corveg:disturbance-age-1 a skos:Concept,
        sosa:Result ;
    skos:definition "<=3 years - recent OR short-term impacts ON STRUCTURE AND composition OF fire still evident" ;
    skos:hiddenLabel "1" ;
    skos:inScheme cv-corveg:disturbance-ages ;
    skos:notation "1" ;
    skos:prefLabel "<=3 years" .

cv-corveg:disturbance-age-2 a skos:Concept,
        sosa:Result ;
    skos:definition ">3 years - evident but short-term impacts not obvious" ;
    skos:hiddenLabel "2" ;
    skos:inScheme cv-corveg:disturbance-ages ;
    skos:notation "2" ;
    skos:prefLabel "> 3 years" .

cv-corveg:disturbance-age-unrecorded a skos:Concept,
        sosa:Result ;
    skos:definition "Unrecorded." ;
    skos:hiddenLabel "999" ;
    skos:inScheme cv-corveg:disturbance-ages ;
    skos:notation "null" ;
    skos:prefLabel "Unrecorded" .

cv-corveg:disturbance-ages a skos:ConceptScheme ;
    skos:hasTopConcept cv-corveg:disturbance-age-1,
        cv-corveg:disturbance-age-2,
        cv-corveg:disturbance-age-unrecorded ;
    skos:prefLabel "Disturbance age concepts" .

