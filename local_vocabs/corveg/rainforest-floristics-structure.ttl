cv-corveg:rainforest-floristics-structure a owl:Ontology ;
    owl:imports <http://www.w3.org/2004/02/skos/core>,
        sosa: .

cv-corveg:rainforest-floristics-structure-m a skos:Concept,
        sosa:Result ;
    skos:definition "no one or two species (combined) of the dominant stratum contribute more than 50% of the crown cover." ;
    skos:hiddenLabel "1" ;
    skos:inScheme cv-corveg:rainforest-floristics-structures ;
    skos:notation "M" ;
    skos:prefLabel "mixed" .

cv-corveg:rainforest-floristics-structure-s a skos:Concept,
        sosa:Result ;
    skos:definition "one or two species (combined) of dominant stratum contribute more than 50% of the crown cover." ;
    skos:hiddenLabel "2" ;
    skos:inScheme cv-corveg:rainforest-floristics-structures ;
    skos:notation "S" ;
    skos:prefLabel "simple" .

cv-corveg:rainforest-floristics-structure-unrecorded a skos:Concept,
        sosa:Result ;
    skos:definition "Unrecorded." ;
    skos:hiddenLabel "999" ;
    skos:inScheme cv-corveg:rainforest-floristics-structures ;
    skos:notation "null" ;
    skos:prefLabel "Unrecorded" .

cv-corveg:rainforest-floristics-structure-x a skos:Concept,
        sosa:Result ;
    skos:definition "as with 'mixed', no one or two species of dominant stratum contribute more than 50% of the crown cover, but one species is conspicuously abundant (hence Mixed Argyrodendron)." ;
    skos:hiddenLabel "3" ;
    skos:inScheme cv-corveg:rainforest-floristics-structures ;
    skos:notation "X" ;
    skos:prefLabel "mixed + one" .

cv-corveg:rainforest-floristics-structures a skos:ConceptScheme ;
    skos:hasTopConcept cv-corveg:rainforest-floristics-structure-m,
        cv-corveg:rainforest-floristics-structure-s,
        cv-corveg:rainforest-floristics-structure-unrecorded,
        cv-corveg:rainforest-floristics-structure-x ;
    skos:prefLabel "Rainforest floristics structure concepts" .

