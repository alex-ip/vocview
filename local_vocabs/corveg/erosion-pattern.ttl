cv-corveg:erosion-pattern a owl:Ontology ;
    owl:imports <http://www.w3.org/2004/02/skos/core>,
        sosa: .

cv-corveg:erosion-pattern-b a skos:Concept,
        sosa:Result ;
    skos:definition "a combination of the lowest relief classes and the steepest modal terrain slopes." ;
    skos:hiddenLabel "14" ;
    skos:inScheme cv-corveg:erosion-patterns ;
    skos:notation "B" ;
    skos:prefLabel "Badlands" .

