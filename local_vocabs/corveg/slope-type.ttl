cv-corveg:slope-type a owl:Ontology ;
    owl:imports <http://www.w3.org/2004/02/skos/core>,
        sosa: .

cv-corveg:slope-type-c a skos:Concept,
        sosa:Result ;
    skos:definition "area that stands above surrounding terrain, with convex slope." ;
    skos:hiddenLabel "1" ;
    skos:inScheme cv-corveg:slope-types ;
    skos:notation "C" ;
    skos:prefLabel "Crest" .

cv-corveg:slope-type-d a skos:Concept,
        sosa:Result ;
    skos:definition "any depression bounded on all sides by higher ground." ;
    skos:hiddenLabel "8" ;
    skos:inScheme cv-corveg:slope-types ;
    skos:notation "D" ;
    skos:prefLabel "Closed depression" .

cv-corveg:slope-type-f a skos:Concept,
        sosa:Result ;
    skos:definition "any flat area situated below a lower slope or simple slope." ;
    skos:hiddenLabel "6" ;
    skos:inScheme cv-corveg:slope-types ;
    skos:notation "F" ;
    skos:prefLabel "Flat" .

cv-corveg:slope-type-h a skos:Concept,
        sosa:Result ;
    skos:definition "compound landform element comprising a narrow crest and short adjoining slopes, the crest length being less than the width of the landform element." ;
    skos:hiddenLabel "9" ;
    skos:inScheme cv-corveg:slope-types ;
    skos:notation "H" ;
    skos:prefLabel "Hillock" .

cv-corveg:slope-type-l a skos:Concept,
        sosa:Result ;
    skos:definition "situated below mid-slope, and used when upper, mid and lower slopes are distinguishable." ;
    skos:hiddenLabel "4" ;
    skos:inScheme cv-corveg:slope-types ;
    skos:notation "L" ;
    skos:prefLabel "Lower slope" .

cv-corveg:slope-type-m a skos:Concept,
        sosa:Result ;
    skos:definition "situated below upper slope, and used when upper, mid and lower slopes are distinguishable." ;
    skos:hiddenLabel "3" ;
    skos:inScheme cv-corveg:slope-types ;
    skos:notation "M" ;
    skos:prefLabel "Mid-slope" .

cv-corveg:slope-type-r a skos:Concept,
        sosa:Result ;
    skos:definition "compound landform element comprising a narrow crest and short adjoining slopes, the crest length being greater than the width of the landform element." ;
    skos:hiddenLabel "10" ;
    skos:inScheme cv-corveg:slope-types ;
    skos:notation "R" ;
    skos:prefLabel "Ridge" .

cv-corveg:slope-type-s a skos:Concept,
        sosa:Result ;
    skos:definition "situated between crest and flat, and used when upper, mid and lower slopes are not distinguishable." ;
    skos:hiddenLabel "5" ;
    skos:inScheme cv-corveg:slope-types ;
    skos:notation "S" ;
    skos:prefLabel "Simple slope" .

cv-corveg:slope-type-u a skos:Concept,
        sosa:Result ;
    skos:definition "situated below the crest, and used when upper, mid and lower slopes are distinguishable." ;
    skos:hiddenLabel "2" ;
    skos:inScheme cv-corveg:slope-types ;
    skos:notation "U" ;
    skos:prefLabel "Upper slope" .

cv-corveg:slope-type-unrecorded a skos:Concept,
        sosa:Result ;
    skos:definition "Unrecorded." ;
    skos:hiddenLabel "999" ;
    skos:inScheme cv-corveg:slope-types ;
    skos:notation "null" ;
    skos:prefLabel "Unrecorded" .

cv-corveg:slope-type-v a skos:Concept,
        sosa:Result ;
    skos:definition "a depression of a linear nature e.g. watercourse." ;
    skos:hiddenLabel "7" ;
    skos:inScheme cv-corveg:slope-types ;
    skos:notation "V" ;
    skos:prefLabel "Open depression" .

cv-corveg:slope-types a skos:ConceptScheme ;
    skos:hasTopConcept cv-corveg:slope-type-c,
        cv-corveg:slope-type-d,
        cv-corveg:slope-type-f,
        cv-corveg:slope-type-h,
        cv-corveg:slope-type-l,
        cv-corveg:slope-type-m,
        cv-corveg:slope-type-r,
        cv-corveg:slope-type-s,
        cv-corveg:slope-type-u,
        cv-corveg:slope-type-unrecorded,
        cv-corveg:slope-type-v ;
    skos:prefLabel "Slope type concepts" .

