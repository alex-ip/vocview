cv-corveg:site-sample-floristics a owl:Ontology ;
    owl:imports <http://www.w3.org/2004/02/skos/core>,
        sosa: .

cv-corveg:site-sample-floristics-a a skos:Concept,
        sosa:Procedure ;
    skos:definition "Complete list of all species present within the site." ;
    skos:hiddenLabel "1" ;
    skos:inScheme cv-corveg:site-sample-floristicss ;
    skos:notation "A" ;
    skos:prefLabel "Complete List" .

cv-corveg:site-sample-floristics-b a skos:Concept,
        sosa:Procedure ;
    skos:definition "List of all woody species present within the site (trees, shrubs, climbers)." ;
    skos:hiddenLabel "2" ;
    skos:inScheme cv-corveg:site-sample-floristicss ;
    skos:notation "B" ;
    skos:prefLabel "Woody Species" .

cv-corveg:site-sample-floristics-c a skos:Concept,
        sosa:Procedure ;
    skos:definition "Woody species plus perennial herbs (trees, shrubs, climbers plus perennial herbs)." ;
    skos:hiddenLabel "3" ;
    skos:inScheme cv-corveg:site-sample-floristicss ;
    skos:notation "C" ;
    skos:prefLabel "Woody Species and Perennial Herbs" .

cv-corveg:site-sample-floristics-d a skos:Concept,
        sosa:Procedure ;
    skos:definition "Dominant characteristic species only/incomplete record of species presented." ;
    skos:hiddenLabel "4" ;
    skos:inScheme cv-corveg:site-sample-floristicss ;
    skos:notation "D" ;
    skos:prefLabel "Dominant characteristic species" .

cv-corveg:site-sample-floristics-e a skos:Concept,
        sosa:Procedure ;
    skos:definition "Other (project-specific, such as weed survey)." ;
    skos:hiddenLabel "5" ;
    skos:inScheme cv-corveg:site-sample-floristicss ;
    skos:notation "E" ;
    skos:prefLabel "Other" .

cv-corveg:site-sample-floristics-unrecorded a skos:Concept,
        sosa:Procedure ;
    skos:definition "Unrecorded." ;
    skos:hiddenLabel "999" ;
    skos:inScheme cv-corveg:site-sample-floristicss ;
    skos:notation "null" ;
    skos:prefLabel "Unrecorded" .

cv-corveg:site-sample-floristicss a skos:ConceptScheme ;
    skos:hasTopConcept cv-corveg:site-sample-floristics-a,
        cv-corveg:site-sample-floristics-b,
        cv-corveg:site-sample-floristics-c,
        cv-corveg:site-sample-floristics-d,
        cv-corveg:site-sample-floristics-e,
        cv-corveg:site-sample-floristics-unrecorded ;
    skos:prefLabel "Site sample floristics concepts" .

