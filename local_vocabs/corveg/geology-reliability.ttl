cv-corveg:geology-reliability a owl:Ontology ;
    owl:imports <http://www.w3.org/2004/02/skos/core>,
        sosa: .

cv-corveg:geology-reliability-h a skos:Concept ;
    skos:definition "High reliability i.e. determined by a pedologist or specialist." ;
    skos:hiddenLabel "1" ;
    skos:inScheme cv-corveg:geology-reliabilitys ;
    skos:notation "H" ;
    skos:prefLabel "High" .

cv-corveg:geology-reliability-l a skos:Concept ;
    skos:definition "Low reliability i.e. determined from a map, or in the field by a non-geologist." ;
    skos:hiddenLabel "3" ;
    skos:inScheme cv-corveg:geology-reliabilitys ;
    skos:notation "L" ;
    skos:prefLabel "Low" .

cv-corveg:geology-reliability-m a skos:Concept ;
    skos:definition "Medium reliability i.e. determined by experienced non-pedologist, or geological type obvious." ;
    skos:hiddenLabel "2" ;
    skos:inScheme cv-corveg:geology-reliabilitys ;
    skos:notation "M" ;
    skos:prefLabel "Medium" .

cv-corveg:geology-reliability-unrecorded a skos:Concept ;
    skos:definition "Unrecorded." ;
    skos:hiddenLabel "999" ;
    skos:inScheme cv-corveg:geology-reliabilitys ;
    skos:notation "null" ;
    skos:prefLabel "Unrecorded" .

cv-corveg:geology-reliabilitys a skos:ConceptScheme ;
    skos:hasTopConcept cv-corveg:geology-reliability-h,
        cv-corveg:geology-reliability-l,
        cv-corveg:geology-reliability-m,
        cv-corveg:geology-reliability-unrecorded ;
    skos:prefLabel "Geology reliability concepts" .

