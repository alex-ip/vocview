cv-corveg:community-extent a owl:Ontology ;
    owl:imports <http://www.w3.org/2004/02/skos/core>,
        sosa: .

cv-corveg:community-extent-a a skos:Concept,
        sosa:Result ;
    skos:definition "<35m wide." ;
    skos:hiddenLabel "1" ;
    skos:inScheme cv-corveg:community-extents ;
    skos:notation "A" ;
    skos:prefLabel "< 35m wide" .

cv-corveg:community-extent-b a skos:Concept,
        sosa:Result ;
    skos:definition "35-75m." ;
    skos:hiddenLabel "2" ;
    skos:inScheme cv-corveg:community-extents ;
    skos:notation "B" ;
    skos:prefLabel "35-75m" .

cv-corveg:community-extent-c a skos:Concept,
        sosa:Result ;
    skos:definition "75-150m." ;
    skos:hiddenLabel "3" ;
    skos:inScheme cv-corveg:community-extents ;
    skos:notation "C" ;
    skos:prefLabel "75-150m" .

cv-corveg:community-extent-d a skos:Concept,
        sosa:Result ;
    skos:definition "150-300m." ;
    skos:hiddenLabel "4" ;
    skos:inScheme cv-corveg:community-extents ;
    skos:notation "D" ;
    skos:prefLabel "150-300m" .

cv-corveg:community-extent-e a skos:Concept,
        sosa:Result ;
    skos:definition "> 300m." ;
    skos:hiddenLabel "5" ;
    skos:inScheme cv-corveg:community-extents ;
    skos:notation "E" ;
    skos:prefLabel "> 300m" .

cv-corveg:community-extent-f a skos:Concept,
        sosa:Result ;
    skos:definition "Not linear." ;
    skos:hiddenLabel "6" ;
    skos:inScheme cv-corveg:community-extents ;
    skos:notation "F" ;
    skos:prefLabel "Not linear" .

cv-corveg:community-extent-unrecorded a skos:Concept,
        sosa:Result ;
    skos:definition "Unrecorded." ;
    skos:hiddenLabel "999" ;
    skos:inScheme cv-corveg:community-extents ;
    skos:notation "null" ;
    skos:prefLabel "Unrecorded" .

cv-corveg:community-extents a skos:ConceptScheme ;
    skos:hasTopConcept cv-corveg:community-extent-a,
        cv-corveg:community-extent-b,
        cv-corveg:community-extent-c,
        cv-corveg:community-extent-d,
        cv-corveg:community-extent-e,
        cv-corveg:community-extent-f,
        cv-corveg:community-extent-unrecorded ;
    skos:prefLabel "Community extent concepts" .

